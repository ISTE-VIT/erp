<body>
    <div class="rectangle">
      <div id="M">
            <a href="/profile">
              <p class="panel" style="margin-top: 7vw !important;">Profile</p>
            </a>
                <a href="/stassign">
            <p class="panel" >Assignments</p>
          </a>
          <a href="/stresources">
            <p class="panel" >Resources</p>
          </a>
          <a href="/feepay">
            <p class="panel" >Payment</p>
          </a>
        </div>
