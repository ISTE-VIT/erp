            <a href="/Stumessages">
            <p class="panel">Class Messages</p></a>
        </div>
    </div>
    </div>         
    <div class="verticle">
    </div>

    <h4 style="color: black;
    font-size: 1.95vw;
    margin-left: 27vw !important;
    margin-top: 2vw;"><strong>MARKS</strong></h4>

    <div class="horizontal">
    </div>
    <% for(var i=0;i<Marks.length;i++){ %>


    
    <div class="card " id="CaRd">
      <div class="card-header" style="font-weight: 500; font-size: 1.1vw; border-width: medium;">Data Structure and Algorithms</div>
      <div class="card-body">
        <table>
          <tbody><tr>
            <th></th>
            <th>Max. Marks</th>
            <th>Marks Scored</th>
            <th>Weightage Score</th>
          </tr>
          <tr>
            <td style="text-align: left;">Digital Assignment 1</td>
            <td>10</td>
            <td><%= Marks[i].da1 %></td>
            <td></td>
          </tr>
          <tr>
            <td style="text-align: left;">Digital Assignment 2</td>
            <td>10</td>
            <td><%= Marks[i].da2 %></td>
            <td></td>
          </tr>
          <tr>
            <td style="text-align: left;">Mid Semester Exam</td>
            <td>50</td>
            <td><%= Marks[i].midsem %></td>
            <td></td>
          </tr>
          <tr>
            <td style="text-align: left;">Final Exam</td>
            <td>100</td>
            <td><%= Marks[i].fat %></td>
            <td></td>
          </tr>
        </tbody></table>
      </div>
        
        </p>
      </div>
    </div>
    <% } %>
